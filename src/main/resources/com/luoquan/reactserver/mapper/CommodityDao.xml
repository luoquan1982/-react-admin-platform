<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luoquan.reactserver.mapper.CommodityDao">
    <select id="listCommodities" resultType="com.luoquan.reactserver.entity.Commodity">
        SELECT
            id,
            category_id,
            p_category_id,
            `name`,
            price,
            detail,
            `desc`
        FROM
            commodity
    </select>
</mapper>